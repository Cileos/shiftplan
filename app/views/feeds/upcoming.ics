cal.method_property = ':PUBLISH' # ri_cal bug causes missing ':'
cal.add_x_property 'X-WR-CALNAME', 'Clockwork'
@schedulings.map(&:decorate).each do |s|
  cal.event do
    dtstart      s.starts_at
    dtend        s.ends_at
    summary      s.summary
    uid          "#{s.class.name}-#{s.id}@#{Volksplaner.hostname}"
    last_modified s.updated_at
  end
end
