- schedulings.each do |scheduling|
  %div.vertical.scheduling{class: scheduling.css_class, data: scheduling.metadata.merge(filter.stack_metadata_for(scheduling))}
    = scheduling.work_time
    = scheduling.link_to_conflicts
    - if scheduling.employee
      - scheduling.employee.organization_id = current_organization.id
      = render 'employees/tiny', employee: scheduling.employee
    - if qualification = scheduling.try(:qualification)
      %span.qualification_name
        = qualification.name
    -# = avatar(scheduling.employee.user, scheduling.employee, :tiny)
    - if scheduling.team
      = abbr_tag scheduling.team_shortcut, scheduling.team_name, class: 'team_name'
