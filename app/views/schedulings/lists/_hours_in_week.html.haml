- schedulings.each do |scheduling|
  %div.vertical.scheduling{class: scheduling.css_class, data: scheduling.metadata.merge(filter.stack_metadata_for(scheduling))}
    = abbr_tag scheduling.hour_range_quickie, scheduling.hour_range_with_duration, class: 'work_time ' + scheduling.css_class
    %span.employee_name= scheduling.employee.name
    -# = avatar(scheduling.employee.user, scheduling.employee, :tiny)
    - if scheduling.team
      = abbr_tag scheduling.team_shortcut, scheduling.team_name, class: 'team_name'

    = render 'schedulings/gimmicks', scheduling: scheduling
