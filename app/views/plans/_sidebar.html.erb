<div id="sidebar">
	<h3><%= t(:employees) %></h3>
	<ul id="employees">
	  <%= employees.map { |employee| presenter_for(employee, 'plan/sidebar/employee') } -%>
	</ul>

	<h3><%= t(:workplaces) %></h3>
	<ul id="workplaces">
	  <%= workplaces.map { |workplace| presenter_for(workplace, 'plan/sidebar/workplace') } -%>
	</ul>

  <h3><%= t(:qualifications)%></h3>
	<ul id="qualifications">
	  <%= qualifications.map { |qualification| presenter_for(qualification, 'plan/sidebar/qualification') } -%>
		<!--<li><a href="#" class="qualification any">(Beliebig)</a></li>-->
	</ul>
</div>
