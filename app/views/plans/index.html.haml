-title :plans

- if can? :create, Plan
  - masterhead_toolbar do
    =link_to ta(:add), [:new] + nested_resources_for(current_organization) + [:plan], class: 'button button-add', remote: true

#plans
  - unless collection.empty?
    = render 'table', records: collection
  - else
    %p#plans= t(:'.no_plans')
